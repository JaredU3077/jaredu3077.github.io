{
  "name": "DarkWave Demoscene Platform",
  "short_name": "DarkWave",
  "description": "Bleeding-edge demoscene platform with WebGL, procedural audio, and quantum effects",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0d0b1e",
  "theme_color": "#b388ff",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "categories": ["entertainment", "music", "art", "games"],
  "icons": [
    {
      "src": "/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "Quantum Vortex",
      "short_name": "Vortex",
      "description": "Launch the Quantum Vortex demo",
      "url": "/#hero",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Create Demo",
      "short_name": "Create",
      "description": "Open the demo creation tools",
      "url": "/#create",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Showcase",
      "short_name": "Demos",
      "description": "Browse demo showcase",
      "url": "/#showcase",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshot-wide.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "DarkWave Demoscene Platform - Main Interface"
    },
    {
      "src": "/screenshot-narrow.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "DarkWave Demoscene Platform - Mobile View"
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+darkwave",
      "url": "/?demo=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/json": [".json"],
        "text/javascript": [".js"]
      }
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["application/json", "text/javascript"]
        }
      ]
    }
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "permissions": [
    "audio-capture",
    "video-capture",
    "fullscreen",
    "notifications",
    "background-sync"
  ],
  "background": {
    "service_worker": "/sw.js",
    "type": "module"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": ["/js/*", "/css/*", "/assets/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "externally_connectable": {
    "matches": ["*://*.darkwave-demo.com/*"]
  },
  "chrome_settings_overrides": {
    "homepage": "/"
  },
  "chrome_url_overrides": {
    "newtab": "/"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      },
      "description": "Open DarkWave Demoscene Platform"
    }
  },
  "options_page": "/options.html",
  "homepage_url": "https://darkwave-demo.com",
  "author": "DarkWave Team",
  "version": "1.0.0",
  "minimum_chrome_version": "88",
  "update_url": "https://darkwave-demo.com/updates.xml"
} 